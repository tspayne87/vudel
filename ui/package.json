{
  "name": "@vudel/ui",
  "version": "0.1.5",
  "description": "Set of components used to build out elements for forms and data sets",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register 'tests/**/*.spec.ts'",
    "start": "vite",
    "build": "npm run clean && rollup -c && npm run generate:declaration",
    "generate:declaration": "tsc --outDir types --declaration true --module esnext --emitDeclarationOnly --project tsconfig.build.json",
    "clean": "rimraf dist && rimraf types"
  },
  "author": "Terry Payne",
  "license": "MIT",
  "files": [
    "dist",
    "types",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@vudel/core": "^0.1.11",
    "@vudel/validations": "^0.1.10",
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@vue/compiler-sfc": "^3.0.4",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.5",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2",
    "vite": "^1.0.0-rc.4",
    "vue-runtime-helpers": "^1.1.2"
  }
}
