{
  "name": "@vudel/validations",
  "version": "0.1.11",
  "description": "Basic Validation library that takes a field set and configures validation for that field",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "types/index.d.ts",
  "homepage": "https://github.com/tspayne87/vudel/tree/main/validations",
  "repository": {
    "url": "https://github.com/tspayne87/vudel",
    "type": "git"
  },
  "scripts": {
    "test": "mocha -r ts-node/register 'tests/**/*.spec.ts'",
    "build": "npm run clean && rollup -c && npm run generate:declaration",
    "generate:declaration": "tsc --outDir types --declaration true --module esnext --emitDeclarationOnly",
    "clean": "rimraf dist && rimraf types"
  },
  "author": "Terry Payne",
  "license": "MIT",
  "files": [
    "dist",
    "types",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@vudel/core": "^0.1.11",
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.28.1",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  }
}
